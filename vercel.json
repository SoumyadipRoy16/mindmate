{
    "version":2,
    "builds": [
        {
            "src":"./index.py",
            "use": "@vercel/python",
            "config":{"runtime":"python3.8", "maxLambdaSize": "450mb"}
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ]
}
